package BasicPgm;

public class NoOfRepe {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[]= {1, 2, 1, 3, 4, 5, 5};
		int number = 2, count =0;
		boolean status = false;
		
		for(int x:a)
		{
			if(x==number)
			{
				count++;
			}
		}System.out.println("Element found and repeated " +count  + "  "+ "times");
		if(status==false)
		{
			System.out.println("Element not found in Array ");
		}

	}

}
