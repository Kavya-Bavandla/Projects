package Learning;

public class PG4PrimeNumber {

	public static void main(String[] args) {
		
		
		
		int count=0;
	
		for(int i=1;i<=10;i++)
		{
			count=0;
			for(int j=i;j<=10;j++)
			{
				if(j%i==0)
				count++;
			}

		if(count==2)
		{
			System.out.println("prime number" +i);
		}else
		{
			System.out.println("not prime" + i);
		}

		}
	}

}
