package RD_Learning;

import java.util.Scanner;


public class PrintABCDtoAABBCCDD {

	public static void main(String[] args) {
		
		
		
		String s ="ABCD";
		
		StringBuilder res = new StringBuilder();
		
		
		for(int i=0;i<s.length();i++)
		{
			char c =s.charAt(i);
			res=res.append(c).append(c);
		}
	}

}
