package RD_Learning;


interface I1
{
	int a;
	void kavya();
	
	default void m1()
	{
		System.out.println("this is default method of m1 interface i1");
	}
	
	static void m2()
	{
		System.out.println("this is default method of m1 interface i1");
	}
	
	
	
}
interface I2
{
	void kavya();
	default void m1()
	{
		System.out.println("this is default method of m1 interface i2");
	}
	
	
	static void m2()
	{
		System.out.println("this is default method of m1 interface i2");
	}
	

}
public class Hello implements  I1, I2
{
	public final String s1 ;
	
	public Hello(String s1) {
		this.s1=s1;
	}
	public static void main(String[] args) {
		

		Hello c = new Hello();
		c.kavya();
		c.m1();
		I1.m2();
		I2.m2();
	}

	@Override
	public void kavya() {
		System.out.println("avya methid");
		
	}

	@Override
	public void m1() {
		// TODO Auto-generated method stub
		I1.super.m1();
		I2.super.m1();
		
		
	}
	




}
