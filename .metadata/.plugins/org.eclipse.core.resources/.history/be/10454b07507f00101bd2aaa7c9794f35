package BasicPgm;

public class CheckVowelPresentOrNot {
	
	public static void main(String[] args)
	{
		String s="Kavya";
		
		for(int i =0;i<=s.length();i++)
		{
			char ch=s.charAt(i);
			if(ch=='a' || ch=='e' || ch=='i' || ch == 'o' || ch=='u')
			{
				System.out.println("Vowel is present");
			}else
			{
				System.out.println("vowel not present");
			}
		}
	}

}
