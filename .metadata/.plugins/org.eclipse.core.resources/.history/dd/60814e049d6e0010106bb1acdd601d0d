package BasicPgm;

public class SortingAnArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int a[]= {3, 1, 2}, temp=0;
		
		for(int i=0;i<=a.length-1;i++)
		{
			if(a[i]>a[i+1])
			{
				temp=a[i];
				a[i]=a[i+1];
				a[i+1]=temp;
				
			}i=-1;
		}
	}

}
